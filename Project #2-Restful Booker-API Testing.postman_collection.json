							"    \r",
							"    pm.expect(responseData.totalprice).to.be.a('number');\r",
							"    pm.expect(responseData.totalprice).to.be.above(0);\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"Bookingdates contain valid checkin and checkout dates\", function () {\r",
							"    const responseData = pm.response.json();\r",
